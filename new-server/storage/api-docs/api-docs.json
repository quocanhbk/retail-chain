                        }
                    }
                },
                "type": "object"
            },
            "UpdateBranchInput": {
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "address": {
                        "type": "string"
                    },
                    "image": {
                        "type": "string",
                        "format": "binary"
                    }
                },
                "type": "object"
            },
            "TransferredEmployeeInput": {
                "required": [
                    "id",
                    "roles"
                ],
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "roles": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "type": "object"
            },
            "NewEmployeeInput": {
                "required": [
                    "name",
                    "email",
                    "password",
                    "roles"
                ],
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    },
                    "roles": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "phone": {
                        "type": "string"
                    },
                    "birthday": {
                        "type": "string"
                    },
                    "gender": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "Employee": {
                "required": [
                    "id",
                    "name",
                    "email",
                    "store_id",
                    "created_at",
                    "updated_at"
                ],
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "store_id": {
                        "type": "integer"
                    },
                    "avatar": {
                        "type": "string"
                    },
                    "avatar_key": {
                        "type": "string"
                    },
                    "phone": {
                        "type": "string"
                    },
                    "birthday": {
                        "type": "string"
                    },
                    "gender": {
                        "type": "string"
                    }
                },
                "type": "object",
                "oneOf": [
                    {
                        "$ref": "#/components/schemas/UpsertTime"
                    }
